<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.gameproject.MPGameStageController"
            prefHeight="400.0" prefWidth="600.0" fx:id="MPGameStageBody">
    <Label fx:id="playerTitleHost"/>
    <Rectangle fx:id="barContainHost"/>
    <Rectangle fx:id="barRecHost"/>
    <Rectangle fx:id="healthRecHost"/>
    <AnchorPane fx:id="heartIconHost"/>

    <Text fx:id="playerTitleJoined"/>
    <Rectangle fx:id="barContainJoined"/>
    <Rectangle fx:id="barRecJoined"/>
    <Rectangle fx:id="healthRecJoined"/>
    <AnchorPane fx:id="heartIconJoined"/>

    <ImageView fx:id="hostSprite" pickOnBounds="true"/>
    <ImageView fx:id="joinedSprite" pickOnBounds="true"/>
    <Rectangle fx:id="rec"/>

</AnchorPane>
